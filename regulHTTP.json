        "name": "Fire Alert Script",
        "order": 4,
        "width": "1",
        "height": "1",
        "format": "<script>\n    (function(scope) {\n    scope.$watch('msg', function(msg) {\n        if (msg && msg.payload && msg.payload.status && msg.payload.status.fire !== undefined) {\n            const body = document.querySelector('body');\n            \n            if (msg.payload.status.fire === true) {\n                body.classList.add('fire-alert');\n                console.log('Fire alert ACTIVÉE');\n                \n                // Vérifier après un court délai\n                setTimeout(function() {\n                    const computedStyle = window.getComputedStyle(body);\n                    console.log('Animation appliquée:', computedStyle.animationName);\n                    console.log('Classes du body:', body.className);\n                }, 100);\n            } else {\n                body.classList.remove('fire-alert');\n                console.log('Fire alert DÉSACTIVÉE');\n            }\n        }\n    });\n})(scope);\n</script>",
